{
	"name": "samvieten.com",
	"version": "1.0.0",
	"private": true,
	"description": "buildprocess for samvieten.com",
	"main": "index.js",
	"scripts": {
		"build": "yarn sass && yarn js",
		"sass": "node-sass ./src/style/*.sass -o ./docs/build/modules",
		"js": "uglifyjs ./src/js/*.js -o ./docs/build/app.js",
		"watch": "watch -p ./src/style/*.sass -p ./src/js/*.js -c \"yarn build\""
	},
	"author": "sam vieten",
	"license": "not granted",
	"devDependencies": {
		"node-sass": "^4.5.3",
		"uglify-es": "^3.1.3",
		"watch-cli": "^0.2.2"
	}
}
