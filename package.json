{
	"name": "samvieten.com",
	"version": "1.0.0",
	"private": true,
	"description": "buildprocess for samvieten.com",
	"main": "index.js",
	"scripts": {
		"build": "npm run sass && npm run js",
		"sass": "node-sass src/style/ -o docs/build/ --output-style=compressed",
		"js": "uglifyjs src/js/*.js -o docs/build/app.js",
		"watch": "watch -p 'src/style/*.scss' -p 'src/js/*.js' -c 'npm run build'"
	},
	"author": "sam vieten",
	"license": "not granted",
	"devDependencies": {
		"node-sass": "^4.9.0",
		"uglify-es": "^3.3.9",
		"watch-cli": "^0.2.3"
	}
}
